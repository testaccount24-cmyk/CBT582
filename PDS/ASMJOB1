//SPGSRST JOB (TS,2322),'TECH.SUPP-MON SANTOS',CLASS=A,NOTIFY=SPGSRS,
//        MSGLEVEL=(1,1),MSGCLASS=A
//***********************************************************
//*      SAMPLE JCL TO ASSEMBLE AND LINKEDIT THE TAPEMAP    *
//*      PROGRAM.                                           *
//***********************************************************
//ASM     EXEC PGM=IFOX00,REGION=1024K,
//             PARM=(TERM,OBJ,LOAD,NODECK)
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//         DD  DSN=SPGSRS.TAPEMAP.ASM,DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(15,5)),
//             DISP=(,PASS)
//SYSUT2   DD  UNIT=SYSDA,SPACE=(CYL,(15,5))
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(15,5))
//SYSTERM  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*,DCB=BLKSIZE=1089
//SYSGO    DD  DSN=&&OBJSET,UNIT=SYSDA,SPACE=(CYL,(1,1),RLSE),
//             DISP=(MOD,PASS)
//SYSIN    DD  DSN=SPGSRS.TAPEMAP.ASM(TAPEMAP),DISP=SHR
//*
//LKED    EXEC PGM=IEWL,REGION=1024K,
//             PARM='XREF,LET,LIST,NCAL',
//             COND=(0,LT)
//SYSLMOD  DD  DSN=SYS2.LINKLIB(TAPEMAP),DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,DISP=(OLD,DELETE)
//SYSPRINT DD  SYSOUT=*
//SYSLIN   DD  DSN=&&OBJSET,DISP=(OLD,DELETE)
//
